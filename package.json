{
  "name": "react-lines-ellipsis",
  "version": "0.14.1",
  "description": "Simple multiline ellipsis component for React.JS",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "standard && jest",
    "lint:fix": "standard --fix",
    "prebuild": "mkdirp lib",
    "build": "babel src --out-dir lib",
    "docs": "browserify -e docs/app.js --transform-key=docs | uglifyjs -m -b max_line_len=800,beautify=false > docs/app.bundle.js",
    "docs:watch": "watchify -e docs/app.js --transform-key=docs -o docs/app.bundle.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "react-component",
    "multiline",
    "ellipsis",
    "dotdotdot",
    "clamp",
    "read-more"
  ],
  "author": "xiaody",
  "license": "MIT",
  "repository": "xiaody/react-lines-ellipsis",
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "aliasify": "2.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "core-js": "^2.6.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10",
    "jest": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.5",
    "preact": "^8.4.2",
    "raf": "^3.4.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "standard": "^11.0.1",
    "uglify-js": "^3.4.9",
    "watchify": "^3.11.1"
  },
  "standard": {
    "globals": [
      "ResizeObserver"
    ],
    "ignore": [
      "lib",
      "docs/*.bundle.js"
    ]
  },
  "jest": {
    "testURL": "http://localhost/",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupFiles/setupTests.js"
    ]
  },
  "browserify": {
    "docs": [
      "babelify",
      "aliasify"
    ]
  },
  "aliasify": {
    "aliases": {
      "react": "preact"
    }
  }
}
